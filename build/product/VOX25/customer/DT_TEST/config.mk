# exist model for vodafone are RV620, RV620mini
export SC_MODULE = VOX25
export SC_MODULE_RF_TAG =

# Turn off Sercomm GPON QoS for now.
export SCM_GPON_US_QOS = 0

# Enable DVR function 
export SC_DVR = 1
CFLAGS += -DSC_SSID_PREFIX=\"vodafone\"
# NWT requires special CATV parameter define format.
CFLAGS += -DSPECIAL_ARPPING_URI_DEFINE
# Vodafone Motive ACS will setting SIP Auth name using URI "IGD...line.1.AuthName"
# CFLAGS += -DSPECIAL_SIP_AUTH_USER_URI_DEFINE


CFLAGS += -DSUPPORT_RESET_ALL_CONFIG
CFLAGS += -DGPON_PASSWORD_EXTRA_CHECK
CFLAGS += -DLANGUAGE_SUPPORT=\"en\"
export LANGUAGE_SUPPORT = en

export SC_SUPPORT_TWO_FXS = 1
export SUPPORT_ERICSSON_IMS = 1
export CONFIG_SUPPORT_MULTI_LEVEL_CONFIG = 1
export SC_USB_PRINTER = 1
export SC_USB_STORAGE = 1
ifeq ($(SC_USB_STORAGE), 1)
	export CONFIG_SUPPORT_SAMBA_VERSION_36 = 1
endif
CFLAGS += -DCONFIG_SUPPORT_USER_MANAGEMENT_BY_ENDUSER
CFLAGS += -DCONFIG_SUPPORT_WEEK_DATE
export SC_MANUFACTUREERURL = www.sercomm.com
CFLAGS += -DSC_MANUFACTUREERURL=\"www.sercomm.com\"
CFLAGS += -DSC_MODELURL=\"www.vodafone.com\"
export SUPPORT_PERLINE_PROVISION=0
export SUPPORT_LONGSHINE_DETECT = 0
export CONFIG_SUPPORT_L2BRIDGING = 0
CFLAGS += -DCONFIG_SUPPORT_MERGE_SAVE_APPLY
CFLAGS += -DCONFIG_SUPPORT_BLOCKING_ACCESS_TO_NEW_DEVICES
CFLAGS += -DCONFIG_SUPPORT_RESERVE_PORTMAPPING_FIRST_ENTRY
CFLAGS += -DWAN_INTERFACE_VISIBLE_ON_IPFILTER
CFLAGS += -DCONFIG_SUPPORT_WIFI_GUEST_FOR_END_USER	
CFLAGS += -DCONFIG_SUPPORT_DHCP_INFO_HOME_PAGE	
CFLAGS += -DHIDE_WIFI_BROADCAST_ON_HOME_PAGE
CFLAGS += -DENABLE_WAN_RESPONSE_PING_ALWAYS
CFLAGS += -DCONFIG_SUPPORT_AUTO_LIMIT
CFLAGS += -DCONFIG_SUPPORT_ARP_TABLE_FILTER
CFLAGS += -DCONFIG_SUPPORT_DYNAMIC_CLI_PROMOTE

